  -
    name : BTN_CONNECTION
    identifier : 0x286d0d4e02c31431
    internal_properties : 
    properties :
     x : 22
     y : 293
     width : 80
     height : 24
    type : 4
    code_elements :
     type_code : 4
     p_codes :
      -
        internal_properties : CAAAAAgAAAACZXalCK8sWVoNA7NyrVkaCMJvNMusOWWfZanXpco7wMY5howQfWcoPikej8PGpPEadlLYMWT1TkDAOT0klTwwfHxZt2FHRcT6P7vSL/TRFkvkcQI1qJupM1RcS4+0aAXLue3IOhcq1ryydGRRwPJr6jWOTBnunGYnNow2cRY=
        code : |1-
         
         // Disables the button 
         MySelf..State = Grayed
         STC_MESSAGE   = "Connect to the service..."
         WinRedraw
         HourGlass(True)
         
         // According to the source
         SWITCH RADIO_SOURCE
         
         	// Twitter
         	CASE 1
         		
         		MySourceOAuth = new MicrosoftLogin
         		
         	// Google
         	CASE 2
         		
         		MySourceOAuth = new GoogleLogin		
         		
         END
         
         // Refresh the message
         STC_MESSAGE   = "Connect to the service " + MySourceOAuth.Lastname + "..."
         WinRedraw
         
         // Starts the connection
         IF MySourceOAuth.Connected() THEN
         	
         	// If the connection was successful
         	IF MySourceOAuth.IsConnected THEN
         		
         		// Refresh the message
         		STC_MESSAGE = "Connected. Reading profile..."
         		WinRedraw
         		HourGlass(True)
         		
         		// Displays the connection source
         		STC_CONNECTED		= StringBuild("You are connected via %1", MySourceOAuth.Lastname)
         		
         		// Retrieves the basic information of the user
         		clInfo is OAuthWebUser = MySourceOAuth.UserInfo
         		
         		// Displays the basic information
         		STC_LOGIN 			= AnsiToUnicode(clInfo.Lastname, charsetUTF8)
         		STC_OTHER_INFO	= (["Email: "] + clInfo.Email) + [TAB] + (["Web site: "] + clInfo.WebSite)
         		
         		// Retrieves the image, there is no need to use OAuthRequest
         		IF clInfo.Image <> "" THEN
         			HTTPRequest(clInfo.Image)
         			IMG_LOGIN = HTTPGetResult(httpResult)
         		ELSE
         			IMG_LOGIN = ""
         		END
         		
         		// Change plane while playing an animation
         		AnimationPrepare(MyWindow, 0, IMG_BACKGROUND..Y + IMG_BACKGROUND..Height, MyWindow..Width, MyWindow..Height)
         		MyWindow..Plane = 2
         		AnimationPlay(animScrollLeft, 25)
         		
         		HourGlass(False)
         	END
         ELSE
         	Error()
         END
         
         // Refresh the message
         STC_MESSAGE 	= ""
         MySelf..State 	= Active
         WinRedraw
         HourGlass(False)
         
         CASE EXCEPTION:
         Error(ExceptionInfo)
         STC_MESSAGE 	= ""
         MySelf..State 	= Active
         WinRedraw
         HourGlass(False)
